        .loading-page {
          position: relative;
          width: 100%;
          height: 100%;
          background-color: #60C7F2;
        }
        .loading-page * {
          color: inherit;
          font: inherit;
        }
        .loading-page .progress {
          position: relative;
          top: 2rem;
          display: block;
          width: 6rem;
          margin: 0 auto;
          text-align: center;
        }
        .loading-page .progress_text {
          display: block;
          font-size: .5rem;
          font-weight: normal;
          line-height: 2;
          color: #d5171d;
        }
        .loading-page .progress_outer-bar {
          display: block;
          padding: 1px;
          border-radius: 4px;
          background-color: #fff;
        }
        .loading-page .progress_inner-bar {
          display: block;
          width: 0;
          height: 6px;
          border-radius: 3px;
          background-color: #d5171d;
        }
        .loading-page .flying-area {
          position: absolute;
          top: 50%;
          width: 10rem;
          height: 6rem;
          margin-top: -3rem;
        }
        .loading-page .clouds {
          -webkit-animation: cloud-move 6s linear infinite;
          animation: cloud-move 6s linear infinite;
        }
        .loading-page .cloud {
          display: block;
          width: 1.5rem;
          height: 1rem;
          background-size: 100% 100%;
        }
        .loading-page .cloud1 {
          -webkit-transform: translate3d(1rem, 0, 0) scale(1, 1);
          transform: translate3d(1rem, 0, 0) scale(1, 1);
        }
        .loading-page .cloud2 {
          -webkit-transform: translate3d(7.3rem, 0.5rem, 0) scale(0.8, 0.8);
          transform: translate3d(7.3rem, 0.5rem, 0) scale(0.8, 0.8);
        }
        .loading-page .cloud3 {
          -webkit-transform: translate3d(2.5rem, 4rem, 0) scale(0.5, 0.5);
          transform: translate3d(2.5rem, 4rem, 0) scale(0.5, 0.5);
        }
        .loading-page .cloud4 {
          -webkit-transform: translate3d(5rem, 2.5rem, 0) scale(0.4, 0.4);
          transform: translate3d(5rem, 2.5rem, 0) scale(0.4, 0.4);
        }
        .loading-page .cloud5 {
