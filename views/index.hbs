<script>
  // 启动页
  try{
    (function () {
      function getURL(url) {
        return 'url("' + url + '")'
      }
      function createCloud(i) {
        var cloudContainer = document.createElement('div')
        cloudContainer.setAttribute('class', 'cloud-container ' + (i < 5 ? '' : 'cloud-container__left'))
        var cloud = document.createElement('span')
        cloud.setAttribute('class', 'cloud cloud' + (i % 5 + 1))
        cloud.style.backgroundImage = getURL(bgCloud)
        cloudContainer.appendChild(cloud)
        return cloudContainer
      }
      function updateView(p) {
        progressNumber.innerText = p
        innerBar.style.width = p + '%'
      }

      var bgEarth = '/serverImg/loading/earth.png'
      var bgPlane = '/serverImg/loading/plane.png'
      var bgPlaneLeaf = '/serverImg/loading/plane-leaf.png'
      var bgCloud = '/serverImg/loading/cloud.png'
      var loadingPage = document.getElementsByClassName('loading-page')[0]
      loadingPage.getElementsByClassName('earth')[0].style.backgroundImage = getURL(bgEarth)
      loadingPage.getElementsByClassName('plane')[0].style.backgroundImage = getURL(bgPlane)
      loadingPage.getElementsByClassName('plane_leaf')[0].style.backgroundImage = getURL(bgPlaneLeaf)
      var flyingArea = loadingPage.getElementsByClassName('flying-area')[0]
      var clouds = document.createElement('div')
      clouds.className = 'clouds'
      for (var i = 0; i < 10; i++) {
        clouds.appendChild(createCloud(i))
      }
      flyingArea.appendChild(clouds)
      var progressNumber = loadingPage.getElementsByClassName('progress_number')[0]
      var innerBar = loadingPage.getElementsByClassName('progress_inner-bar')[0]
      var p = 1
      var intervalId
      window.loadingCallBack = function (pro) {
        if (pro < 100) { // 资源包下载进度
          p = Math.floor((+pro) / 2)
          updateView(p)
        } else if (pro == 100) { // 资源包下载完成，开始下载并执行JS
          if (p > 50) return
          p = 50
          updateView(p)
          clearInterval(intervalId)
          intervalId = setInterval(function () {
            p++
            if (p < 100) {
              updateView(p)
            } else {
              clearInterval(intervalId)
            }
          }, 16)
        } else if (pro < 0) { // 载入主页
          p = 100
          updateView(p)
          clearInterval(intervalId)
        }
      }

    })()
  }catch(e){
  }
</script>

